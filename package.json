{
  "name": "user-management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run lint -- --fix && vue-cli-service serve",
    "build": "npm run lint -- --fix && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "eslint": "eslint -f checkstyle --ext .js,.vue src -o eslint_result.xml"
  },
  "dependencies": {
    "a": "^2.1.2",
    "acorn": "^7.2.0",
    "auth0-js": "9.12.2",
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "deep-extend": "^0.6.0",
    "element-ui": "^2.12.0",
    "elliptic": "^6.5.3",
    "http-proxy": "^1.18.1",
    "http-proxy-agent": "^4.0.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "mavon-editor": "^2.9.0",
    "sass-loader": "^8.0.0",
    "serialize-javascript": "^4.0.0",
    "vue": "^2.6.11",
    "vue-cookies": "^1.7.0",
    "vue-drag-verify": "^1.0.6",
    "vue-generate-component": "^3.0.0",
    "vue-i18n": "^8.15.3",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "less": "^3.10.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/base",
      "plugin:vue/essential",
      "plugin:vue/strongly-recommended",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      },
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
